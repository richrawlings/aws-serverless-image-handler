{
  "name": "richrawlings/craft-aws-serverless-image-handler",
  "description": "Integrates the AWS Serverless Image Handler CDN platform into Craft CMS for supercharged image manipulation and delivery.",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "assets",
    "amazon",
    "aws",
    "cdn",
    "cms",
    "craft",
    "craftcms",
    "craft-plugin",
    "face-recognition",
    "images",
    "serverless",
    "thumbnails"
  ],
  "support": {
    "docs": "https://github.com/richrawlings/craft-aws-serverless-image-handler/wiki/Documentation",
    "issues": "https://github.com/richrawlings/craft-aws-craft-serverless-image-handler/issues"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Richard Rawlings",
      "homepage": "https://richardrawlings.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "craftcms/cms": "^4.0.0-beta.1|^5.0.0-beta.1",
    "aws/aws-sdk-php": "^3.303"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "richrawlings\\awsserverlessimagehandler\\": "src/"
    }
  },
  "extra": {
    "name": "AWS Serverless Image Handler",
    "handle": "aws-serverless-image-handler"
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
